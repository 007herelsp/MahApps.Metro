[bugtraq "issues"]
    url = https://github.com/MahApps/MahApps.Metro/issues/%BUGID%
    logfilterregex = "((#\\d+)+)"
    logregex = \\d+
    
[bugtraq "users"]
    url = https://github.com/%BUGID%
    loglinkregex = @\\w+
    logregex = \\w+
    
[bugtraq "commitId"]
    url = https://github.com/MahApps/MahApps.Metro/commit/%BUGID%
    logregex = [0-9a-f]{6,32}